{
    "name": "ossipesonen/elixir",
    "type": "library",
    "description": "Zero dependency PHP SQL Query Builder extracted from Doctrine DBAL",
    "keywords": [
        "abstraction",
        "database"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4.1"
    },
    "config": {
        "platform": {
            "php": "7.1.0"
        }
    },
    "autoload": {
        "psr-4": {
            "Elixir\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elixir\\Tests\\": "tests/"
        }
    }
}
